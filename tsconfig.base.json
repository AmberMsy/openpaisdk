{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "declaration": true,
        "outDir": "./lib",
        "strict": true,
        "lib": [
            "es2019",
            "DOM"
        ],
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "src",
        "paths": {
            "@pai/*": ["*"],
            "@protocol/*": ["protocol/*"],
            "@api/*": ["api/*"],
            "@commands/*": ["commands/*"]
        }
    }
}
